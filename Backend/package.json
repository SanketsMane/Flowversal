{
  "name": "flowversal-backend",
  "version": "1.0.0",
  "description": "Flowversal Backend API - Node.js with Fastify, MongoDB, Pinecone, and AI integration",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 nodemon",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tsc",
    "start": "node dist/server.js",
    "type-check": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --config jest.config.ts",
    "test:watch": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --config jest.config.ts --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --config jest.config.ts --coverage",
    "test:e2e": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --config jest.e2e.config.js",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "dev:seed": "cross-env NODE_OPTIONS=--max-old-space-size=4096 ts-node src/scripts/dev-utils.ts seed",
    "dev:reset": "cross-env NODE_OPTIONS=--max-old-space-size=4096 ts-node src/scripts/dev-utils.ts reset",
    "dev:health": "cross-env NODE_OPTIONS=--max-old-space-size=4096 ts-node src/scripts/dev-utils.ts health"
  },
  "keywords": [
    "fastify",
    "mongodb",
    "pinecone",
    "ai",
    "workflow",
    "inngest"
  ],
  "author": "",
  "license": "MIT",
  "overrides": {
    "fast-xml-parser": "^5.3.4"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.980.0",
    "@fastify/compress": "^8.3.1",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.9.3",
    "@fastify/websocket": "^8.3.0",
    "@langchain/core": "^1.1.2",
    "@langchain/langgraph": "^1.0.3",
    "@langchain/ollama": "^1.0.3",
    "@langchain/openai": "^1.1.3",
    "@neondatabase/serverless": "^1.0.2",
    "@opentelemetry/api-metrics": "^1.0.0",
    "@pinecone-database/pinecone": "^6.1.3",
    "@sentry/node": "^10.29.0",
    "@supabase/supabase-js": "^2.38.0",
    "@types/speakeasy": "^2.0.10",
    "@types/validator": "^13.15.10",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "fast-xml-parser": "^5.3.4",
    "fastify": "^4.24.3",
    "fastify-plugin": "^5.1.0",
    "inngest": "^3.0.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "langchain": "^1.1.3",
    "mongoose": "^8.0.0",
    "openai": "^4.20.0",
    "prom-client": "^15.1.3",
    "speakeasy": "^2.0.0",
    "stripe": "^20.0.0",
    "validator": "^13.15.23",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@google-cloud/storage": "^7.12.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.4.3",
    "nodemon": "^3.0.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.7.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
